# Environment variables template for BMAD

# Cloud Provider API Keys (optional)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1

AZURE_CLIENT_ID=your_azure_client_id
AZURE_CLIENT_SECRET=your_azure_client_secret
AZURE_TENANT_ID=your_azure_tenant_id

GOOGLE_APPLICATION_CREDENTIALS=path/to/gcp/credentials.json
GOOGLE_CLOUD_PROJECT=your_gcp_project_id

# API Keys for pricing APIs (optional)
CLOUD_PRICING_API_KEY=your_pricing_api_key

# Output preferences
DEFAULT_OUTPUT_FORMAT=markdown
DIAGRAM_OUTPUT_DIR=./output/diagrams
REPORT_OUTPUT_DIR=./output/reports